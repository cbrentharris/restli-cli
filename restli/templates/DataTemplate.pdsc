{
  "type": "{{schema.type}}",
  "name": "{{schema.name}}",
  "namespace": "{{schema.namespace}}",
  "doc": "{{schema.doc}}",
  "fields": [
  {% for field in schema.fields %}
    {"name": "{{field.name}}", "type": "{{field.type}}", "doc": "{{field.doc}}"} {% if not loop.last %},{% endif %}
  {% endfor %}
  ]
}
